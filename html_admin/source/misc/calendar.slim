- content_box_file = '../misc/_content_calendar.slim'

- custom_menu_settings = Hash.new
- custom_menu_settings['color_scheme'] = 'light'
- custom_menu_settings['color_style'] = 'default'
- custom_menu_settings['layout'] = 'mini'
- custom_menu_settings['show_logo_in_menu'] = false

- custom_top_bar_settings = Hash.new
- custom_top_bar_settings['color_scheme'] = 'bright'
- custom_top_bar_settings['show_logo'] = true
- custom_top_bar_settings['above_menu'] = true
- custom_top_bar_settings['show_support_selector'] = true

include ../_partials/_template.slim