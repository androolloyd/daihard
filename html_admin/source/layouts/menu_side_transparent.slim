- content_box_file = '../apps/_content_support_dashboard.slim'

- no_breadcrumbs = true
- all_solid_bg = true
- show_top_bar = true

- custom_top_bar_settings = Hash.new
- custom_top_bar_settings['color_scheme'] = 'light'
- custom_top_bar_settings['show_support_selector'] = true

// Menu Settings variables
- custom_menu_settings = Hash.new
- custom_menu_settings['color_scheme'] = 'light'
- custom_menu_settings['color_style'] = 'transparent'
- custom_menu_settings['layout'] = 'compact'

include ../_partials/_template.slim